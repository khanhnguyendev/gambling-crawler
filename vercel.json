{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "package.json"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "install": {
      "dest": "/var/task",
      "command": "npm install"
    },
    "build": {},
    "dev": {
      "command": "npm run dev"
    }
  }
  